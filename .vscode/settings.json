{
  "telemetry.telemetryLevel": "off",
  // Window.
  "window.restoreWindows": "none",
  "window.newWindowDimensions": "inherit",
  "window.title": "${rootName}",
  "window.titleBarStyle": "custom",
  "window.dialogStyle": "custom",
  // Editor.
  "editor.minimap.enabled": false,
  "editor.fontFamily": "Jetbrains Mono, SF Mono, monospace",
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.75,
  "editor.codeLens": false,
  "editor.guides.indentation": false,
  "editor.folding": false,
  "editor.overviewRulerBorder": false,
  "editor.tabSize": 2,
  "editor.stickyScroll.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.renderLineHighlight": "none",
  "editor.lightbulb.enabled": "off",
  "editor.parameterHints.enabled": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  // Others.
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.enableTrash": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true
  },
  // Workbench.
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 12,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  // Theme.
  "workbench.colorTheme": "Monokai",
  "workbench.iconTheme": "vs-seti",
  // Source Control
  "scm.showIncomingChanges": "never",
  "scm.showOutgoingChanges": "never",
  "git.openRepositoryInParentFolders": "never",
  // Extensions.
  "svg.preview.mode": "svg",
  "vue.codeLens.enabled": false,
  "gitlens.codeLens.authors.command": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.fileAnnotations.command": "blame",
  "gitlens.graph.layout": "editor",
  "gitlens.ai.experimental.generateCommitMessage.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.menus": {
    "editor": false,
    "editorGutter": false,
    "editorTab": false,
    "explorer": false,
    "ghpr": false,
    "scm": false,
    "scmRepositoryInline": false,
    "scmRepository": false,
    "scmGroupInline": false,
    "scmGroup": false,
    "scmItemInline": false,
    "scmItem": false
  },
  "git-graph.commitDetailsView.fileView.type": "File List"
}
